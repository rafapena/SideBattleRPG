<base:_ClassUnstructuredOperations
    x:Class="Database.ClassesUnstructured.OtherLists"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:base="clr-namespace:Database.ClassesUnstructured"
    xmlns:table="clr-namespace:Database.TableTemplates"
    mc:Ignorable="d" 
    d:DesignHeight="300" d:DesignWidth="300"
    Title="OtherLists">
    <Viewbox>
        <StackPanel Orientation="Horizontal" Margin="0 10 0 0">
            <StackPanel.Resources>
                <Style TargetType="table:SingleText">
                    <Setter Property="Margin" Value="10 10 10 10"/>
                </Style>
            </StackPanel.Resources>

            <table:SingleText x:Name="Elements"/>
            <table:SingleText x:Name="WeaponTypes"/>
            <table:SingleText x:Name="SkillTypes"/>

        </StackPanel>
    </Viewbox>
</base:_ClassUnstructuredOperations>
