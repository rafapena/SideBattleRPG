<Page x:Class="Database.Tables.Player"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Database.Tables"
      xmlns:template="clr-namespace:Database.Templates"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Player">
    <StackPanel>
        <template:NavBar x:Name="Header"/>
        
        <StackPanel Orientation="Horizontal">
            <template:TableList x:Name="ObjectList"/>
            <StackPanel Orientation="Vertical">
                <template:BaseObject x:Name="Base"/>
                <StackPanel x:Name="ObjectContent">
                    
                    <!-- Insert here -->
                    
                </StackPanel>
                <template:Footer x:Name="BottomButtons"/>
            </StackPanel>
        </StackPanel>
        
    </StackPanel>
</Page>